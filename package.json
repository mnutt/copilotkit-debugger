{
  "name": "copilotkit-debugger",
  "private": false,
  "homepage": "https://github.com/mnutt/copilotkit-debugger",
  "repository": {
    "type": "git",
    "url": "https://github.com/mnutt/copilotkit-debugger.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.6",
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/index.css"
  },
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch --no-splitting",
    "test": "jest",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf .next",
    "link:global": "pnpm link --global",
    "unlink:global": "pnpm unlink --global"
  },
  "peerDependencies": {
    "react": "^18 || ^19 || ^19.0.0-rc"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.5",
    "@types/react-syntax-highlighter": "^15.5.7",
    "eslint": "^8.56.0",
    "jest": "^29.6.4",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "ts-jest": "^29.1.1",
    "tsup": "^6.7.0",
    "typescript": "^5.2.3"
  },
  "dependencies": {
    "@copilotkit/react-core": "^1.9.0",
    "@copilotkit/runtime-client-gql": "^1.9.0",
    "@copilotkit/shared": "^1.9.0",
    "@headlessui/react": "^2.1.3"
  },
  "keywords": [
    "copilotkit",
    "copilot",
    "react",
    "nextjs",
    "nodejs",
    "ai",
    "assistant",
    "javascript",
    "automation",
    "textarea"
  ]
}
